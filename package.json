{
	"name": "cairjs",
	"displayName": "cairjs",
	"publisher": "siturra",
	"description": "how to create boilerplate from cairjs",
	"version": "1.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/cairjs/cairjs-vscode.git"
	},
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cairjs.createComponent",
		"onCommand:cairjs.createResolver",
		"onCommand:cairjs.createModel"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cairjs.createComponent",
				"title": "Create Component"
			},
			{
				"command": "cairjs.createResolver",
				"title": "Create Resolver"
			},
			{
				"command": "cairjs.createModel",
				"title": "Create Model"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "cairjs.createComponent",
					"enablement": "editorHasSelection",
					"group": "cairJs@1"
				},
				{
					"command": "cairjs.createResolver",
					"enablement": "editorHasSelection",
					"group": "cairJs@2"
				},
				{
					"command": "cairjs.createModel",
					"enablement": "editorHasSelection",
					"group": "cairJs@3"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js",
		"build": "vsce package",
		"publish": "vsce publish"
	},
	"devDependencies": {
		"@types/vscode": "^1.49.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.9.0",
		"eslint-config-standard": "^14.1.1",
		"prettier": "2.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"prettier": {
		"singleQuote": true,
		"semi": false,
		"trailingComma": "all"
	}
}
